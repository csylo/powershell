$colors = @{
  "Command" = "FFFFFF"
  "Comment" = "008000"
  "ContinuationPrompt" = "FFFFFF"
  "DefaultToken" = "FFFFFF"
  "Emphasis" = "85F8FF"
  "Error" = "FE8282"
  "Keyword" = "83FF85"
  "Member" = "FEC682"
  "Number" = "FEC682"
  "Operator" = "ADA295"
  "Parameter" = "ADA295"
  "String" = "ED82FE"
  "Type" = "6AC2FF"
  "Variable" = "FFFF85"
}
Set-PSReadLineOption -Colors $colors
Get-PSReadlineOption
"Profile at $profile"
